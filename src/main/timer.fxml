<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>


<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.TimerController"
      stylesheets="/main/timer.css"
        styleClass="black-background">
    <ToolBar prefHeight="40.0" prefWidth="200.0">
        <Button onAction="#startTimer" styleClass="toolbar-button" text="Start"/>
        <Button onAction="#stopTimer" styleClass="toolbar-button" text="Stop"/>
    </ToolBar>
    <StackPane>
        <GridPane hgap="3" styleClass="black-background" vgap="3">
            <columnConstraints>
                <ColumnConstraints percentWidth="16.667"/>
                <ColumnConstraints percentWidth="16.667"/>
                <ColumnConstraints percentWidth="16.667"/>
                <ColumnConstraints percentWidth="16.667"/>
                <ColumnConstraints percentWidth="16.667"/>
                <ColumnConstraints percentWidth="16.667"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints percentHeight="25"/>
                <RowConstraints percentHeight="25"/>
                <RowConstraints percentHeight="25"/>
                <RowConstraints percentHeight="25"/>
            </rowConstraints>
            <Pane fx:id="paneCol0Row0" styleClass="panel-deactivated"
                  GridPane.columnIndex="0" GridPane.rowIndex="0"/>
            <Pane fx:id="paneCol0Row1" styleClass="panel-deactivated"
                  GridPane.columnIndex="0" GridPane.rowIndex="1"/>
            <Pane fx:id="paneCol0Row2" styleClass="panel-deactivated"
                  GridPane.columnIndex="0" GridPane.rowIndex="2"/>
            <Pane fx:id="paneCol0Row3" styleClass="panel-deactivated"
                  GridPane.columnIndex="0" GridPane.rowIndex="3"/>
            <Pane fx:id="paneCol1Row0" styleClass="panel-deactivated"
                  GridPane.columnIndex="1" GridPane.rowIndex="0"/>
            <Pane fx:id="paneCol1Row1" styleClass="panel-deactivated"
                  GridPane.columnIndex="1" GridPane.rowIndex="1"/>
            <Pane fx:id="paneCol1Row2" styleClass="panel-deactivated"
                  GridPane.columnIndex="1" GridPane.rowIndex="2"/>
            <Pane fx:id="paneCol1Row3" styleClass="panel-deactivated"
                  GridPane.columnIndex="1" GridPane.rowIndex="3"/>
            <Pane fx:id="paneCol2Row0" styleClass="panel-deactivated"
                  GridPane.columnIndex="2" GridPane.rowIndex="0"/>
            <Pane fx:id="paneCol2Row1" styleClass="panel-deactivated"
                  GridPane.columnIndex="2" GridPane.rowIndex="1"/>
            <Pane fx:id="paneCol2Row2" styleClass="panel-deactivated"
                  GridPane.columnIndex="2" GridPane.rowIndex="2"/>
            <Pane fx:id="paneCol2Row3" styleClass="panel-deactivated"
                  GridPane.columnIndex="2" GridPane.rowIndex="3"/>
            <Pane fx:id="paneCol3Row0" styleClass="panel-deactivated"
                  GridPane.columnIndex="3" GridPane.rowIndex="0"/>
            <Pane fx:id="paneCol3Row1" styleClass="panel-deactivated"
                  GridPane.columnIndex="3" GridPane.rowIndex="1"/>
            <Pane fx:id="paneCol3Row2" styleClass="panel-deactivated"
                  GridPane.columnIndex="3" GridPane.rowIndex="2"/>
            <Pane fx:id="paneCol3Row3" styleClass="panel-deactivated"
                  GridPane.columnIndex="3" GridPane.rowIndex="3"/>
            <Pane fx:id="paneCol4Row0" styleClass="panel-deactivated"
                  GridPane.columnIndex="4" GridPane.rowIndex="0"/>
            <Pane fx:id="paneCol4Row1" styleClass="panel-deactivated"
                  GridPane.columnIndex="4" GridPane.rowIndex="1"/>
            <Pane fx:id="paneCol4Row2" styleClass="panel-deactivated"
                  GridPane.columnIndex="4" GridPane.rowIndex="2"/>
            <Pane fx:id="paneCol4Row3" styleClass="panel-deactivated"
                  GridPane.columnIndex="4" GridPane.rowIndex="3"/>
            <Pane fx:id="paneCol5Row0" styleClass="panel-deactivated"
                  GridPane.columnIndex="5" GridPane.rowIndex="0"/>
            <Pane fx:id="paneCol5Row1" styleClass="panel-deactivated"
                  GridPane.columnIndex="5" GridPane.rowIndex="1"/>
            <Pane fx:id="paneCol5Row2" styleClass="panel-deactivated"
                  GridPane.columnIndex="5" GridPane.rowIndex="2"/>
            <Pane fx:id="paneCol5Row3" styleClass="panel-deactivated"
                  GridPane.columnIndex="5" GridPane.rowIndex="3"/>
        </GridPane>
        <Text fx:id="timerText" text="00:00.0"/>
    </StackPane>
</VBox>
